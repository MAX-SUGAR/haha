FAQ.js内容：
//获取地址栏参数
function getUrlParam(name){
	var reg = new RegExp("(^|&)"+ name +"=([^&]*)(&|$)");
	var r = window.location.search.substr(1).match(reg);
	if (r!=null) return unescape(r[2]); return null;
}
function loadType(){
	if(getUrlParam("language")=="engligsh" || getUrlParam("language")=="english"){
		document.getElementById("faq_main_en").style.display="block"; 
	}else{
		document.getElementById("faq_main_ch").style.display="block"; 
		
		var urlQ = window.location.hash;
		
		if(urlQ){
			lookProbrem("wukongMin");
		}
	}
	
	/* var hash = window.location.hash;
	$("body,html").animate({scrollTop:$(hash).offset().top}); */
}
function lookUpProbrem(){
	var ul = $("#wukongProbrem>ul");
	if(ul.is(":hidden")){
		ul.show();
	}else{
		ul.hide();
	}
}
function lookProbrem(transType){
	$("#faq_menu,.contentIfo").hide();
	switch(transType){
		case "app":
			$("#faq_app_content").show();
		break;
		case "search":
			$("#faq_search_content").show();
		break;
		case "wukongMin":
			$("#faq_wukong_min_content").show();
		break;
		case "wukongMax":
			$("#faq_wukong_max_content").show();
		break;
		case "wulian":
			$("#faq_wulian_content").show();
		break;
		case "wuanMgt":
			$("#faq_magnet_content").show();
		break;	
		case "wuanTem":
		$("#faq_tem_content").show();
		break;
		case "wuanIfd":
			$("#faq_infrared_content").show();
		break;			
		case "wuneng":
			$("#faq_wuneng_content").show();
		break;
	}
	$(".returnMenuCon").show();
}
function returnMenu(){
	$(".contentIfo,.returnMenuCon").hide();
	$("#faq_menu").show();
}
window.onload=loadType;
